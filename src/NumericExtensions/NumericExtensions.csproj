<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <TargetFrameworks>netstandard2.0;net5.0;net6.0;</TargetFrameworks>
    <LangVersion>10</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.1.0.0</FileVersion>
    <Version>1.1.0</Version>
    <Authors>Brandon Matheson</Authors>
    <Company />
    <Description>A collection of useful extension methods for numeric types.</Description>
    <PackageProjectUrl>https://github.com/Cityonhill93/NumericExtensions</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Cityonhill93/NumericExtensions</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageReleaseNotes></PackageReleaseNotes>
  </PropertyGroup>

</Project>
